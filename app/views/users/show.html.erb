<div class="profile_content">
  <%= render 'shared/profile_left', { articles: @articles, user: @user} %>

  <div class="profile_right">
    <% if params[:like] == 'true' %>
      <p class="profile_p_title">いいね一覧</p>
      <%= render 'likes', { liked_article_ids: @liked_article_ids } %>
    <% else %>
      <p class="profile_p_title">投稿一覧</p>
      <%= render 'articles', { articles: @articles } %>
    <% end %>
    
  </div>
</div>
